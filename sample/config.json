{
  "env": "sample/variables.env",
  "input": "sample/doc-content.md",
  "output": "sample/results.json",
  "recursive": true,
  "testExtensions": [
    ".md",
    ".mdx",
    ".html",
    ".htm",
    ".xml",
    ".json"
  ],
  "mediaDirectory": "sample",
  "fileTypes": [
    {
      "extensions": [
        ".md",
        ".mdx"
      ],
      "openTestStatement": "[comment]: # (test",
      "closeTestStatement": ")"
    },
    {
      "extensions": [
        ".html",
        ".htm",
        ".xml"
      ],
      "openTestStatement": "<!-- test",
      "closeTestStatement": "-->"
    }
  ],
  "browserOptions": {
    "headless": "true",
    "path": "",
    "height": 600,
    "width": 800
  },
  "analytics": {
    "send": false,
    "userId": "",
    "detailLevel": "action-detailed",
    "customServers": []
  },
  "verbose": false
}