{
  "name": "doc-detective",
  "version": "1.3.1",
  "description": "The documentation testing framework.",
  "main": "src/index.js",
  "scripts": {
    "preinstall": "npm upgrade doc-detective-common",
    "postinstall": "node scripts/postinstall.js",
    "test": "mocha */*.test.js",
    "watch": "chokidar \"**/*.js\" -c \"npm run test\"",
    "appium": "appium"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doc-detective/doc-detective-core.git"
  },
