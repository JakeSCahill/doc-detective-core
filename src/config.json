{
  "env": "",
  "input": ".",
  "output": "./results.json",
  "recursive": true,
  "testExtensions": [
    ".md",
    ".mdx",
    ".html",
    ".htm",
    ".xml",
    ".json"
  ],
  "mediaDirectory": ".",
  "fileTypes": [
    {
      "extensions": [
        ".md",
        ".mdx"
      ],
      "openTestStatement": "[comment]: # (test",
      "closeTestStatement": ")"
    },
    {
      "extensions": [
        ".html",
        ".htm",
        ".xml"
      ],
      "openTestStatement": "<!-- test",
      "closeTestStatement": "-->"
    }
  ],
  "browserOptions": {
    "headless": "true",
    "path": "",
    "height": 800,
    "width": 1200
  },
  "analytics": {
    "send": false,
    "userId": "",
    "detailLevel": "action-detailed",
    "customServers": []
  },
  "logLevel": "info"
}